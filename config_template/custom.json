{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "proxy",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    }
  },
  "dns": {
    "final": "proxyDns",
    "strategy": "prefer_ipv4",
    "independent_cache": true,
    "servers": [
      {
        "tag": "proxyDns",
        "type": "tls",
        "server": "8.8.8.8",
        "detour": "proxy"
      },
      {
        "tag": "localDns",
        "type": "h3",
        "server": "223.5.5.5"
      },
      {
        "type": "fakeip",
        "tag": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "localDns"
      },
      {
        "clash_mode": "global",
        "server": "proxyDns"
      },
      {
        "query_type": ["A", "AAAA"],
        "rule_set": ["bing", "geosite-geolocation-!cn"],
        "server": "fakeip"
      },
      {
        "rule_set": ["bing", "geosite-geolocation-!cn"],
        "server": "proxyDns"
      },
      {
        "rule_set": ["geosite-cn"],
        "server": "localDns"
      },
      {
        "rule_set": ["geoip-cn"],
        "server": "proxyDns",
        "client_subnet": "114.114.114.114/24"
      }
    ]
  },
  "inbounds": [
    {
      "tag": "tun",
      "type": "tun",
      "address": ["172.19.0.1/30", "fdfe:dcba:9876::1/126"],
      "auto_route": true,
      "strict_route": true,
      "stack": "system",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2080
        }
      }
    },
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 2080,
      "users": []
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": ["auto", "direct-out", "{all}"]
    },
    {
      "tag": "auto",
      "type": "urltest",
      "outbounds": ["{all}"],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "type": "direct",
      "tag": "direct-out"
    }
  ],
  "route": {
    "default_domain_resolver": {
      "server": "localDns",
      "rewrite_ttl": 60
    },
    "auto_detect_interface": true,
    "final": "proxy",
    "rules": [
      {
        "inbound": ["mixed", "tun"],
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "inbound": ["mixed", "tun"],
        "action": "resolve",
        "strategy": "prefer_ipv4"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct-out"
      },
      {
        "clash_mode": "global",
        "outbound": "proxy"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": "stun"
          }
        ],
        "action": "reject"
      },
      {
        "rule_set": ["bing", "geosite-geolocation-!cn"],
        "outbound": "proxy"
      },
      {
        "rule_set": ["geosite-cn", "geoip-cn"],
        "outbound": "direct-out"
      }
    ],
    "rule_set": [
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geosite-geolocation-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-!cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "bing",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/bing.srs",
        "download_detour": "proxy"
      }
    ]
  }
}
